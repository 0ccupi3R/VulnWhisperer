{
  "order": 0,
  "index_patterns": "logstash-vulnwhisperer-*",
  "mappings": {
    "doc": {
      "properties": {
        "plugin_id": {
          "type": "integer"
        },
        "last_updated": {
          "type": "date"
        },
        "geoip": {
          "dynamic": true,
          "type": "object",
          "properties": {
            "ip": {
              "type": "ip"
            },
            "latitude": {
              "type": "float"
            },
            "location": {
              "type": "geo_point"
            },
            "longitude": {
              "type": "float"
            }
          }
        },
        "risk_score": {
          "type": "float"
        },
        "source": {
          "type": "keyword"
        },
        "synopsis": {
          "type": "keyword"
        },
        "see_also": {
          "type": "keyword"
        },
        "@timestamp": {
          "type": "date"
        },
        "cve": {
          "type": "keyword"
        },
        "solution": {
          "type": "keyword"
        },
        "port": {
          "type": "integer"
        },
        "host": {
          "type": "text"
        },
        "@version": {
          "type": "keyword"
        },
        "risk": {
          "type": "keyword"
        },
        "assign_ip": {
          "type": "ip"
        },
        "cvss": {
          "type": "float"
        }
      }
    }
  }
}